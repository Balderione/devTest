    <validationRules>
        <fullName>VRER01_Loop_Ref_Cannot_Be_Changed</fullName>
        <active>true</active>
        <description>The Loop Reference (used to sync with PPS) cannot be changed otherwise it will create a new loop on PPS Side</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c),
ISCHANGED( ER_External_Reference__c ))</errorConditionFormula>
        <errorMessage>The Loop Reference field (used to sync with PPS) cannot be changed otherwise it will create a new loop on PPS Side</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER02_Loop_Ref_Caracters_Control</fullName>
        <active>true</active>
        <description>A specific format has to be used, for PPS “Must be Alphanumeric with no spaces. Underscores &apos;_&apos; and dashes &apos;-&apos; are allowed.”</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c),
NOT(REGEX( ER_External_Reference__c ,&quot;^[a-zA-Z0-9_-]*$&quot;)))</errorConditionFormula>
        <errorDisplayField>ER_External_Reference__c</errorDisplayField>
        <errorMessage>The External Reference field cannot contains special Caracters other than - (dash) and _ (underscore)</errorMessage>
    </validationRules>
