    <validationRules>
        <fullName>VRIT02_InvoiceDetail</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
NOT(ISCHANGED(IT_SkipActivation__c)),

OR (
 ISPICKVAL( IT_Financial_Center__r.IT_Service__c , &apos;1L&apos;) ,
 ISPICKVAL( IT_Financial_Center__r.IT_Service__c , &apos;1M&apos;),
 ISPICKVAL( IT_Financial_Center__r.IT_Service__c , &apos;1N&apos;),
 ISPICKVAL( IT_Financial_Center__r.IT_Service__c , &apos;1O&apos;),
 ISPICKVAL( IT_Financial_Center__r.IT_Service__c , &apos;1X&apos;),
 ISPICKVAL( IT_Financial_Center__r.IT_Service__c , &apos;2X&apos;)),

OR((ISPICKVAL(IT_Invoice_Detail__c, &apos;0&apos;)), 

(ISPICKVAL(IT_Invoice_Detail__c, &apos;1&apos;)) , 

(ISPICKVAL(IT_Invoice_Detail__c, &apos;4&apos;))

))</errorConditionFormula>
        <errorMessage>For the type of service, it&apos;s possible to select only no cut invoice detail!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT03_Contract_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( IT_Contract_Type__c , &apos;03&apos;),

OR(
 NOT( ISBLANK(IT_Delivery_Term__c)) ,
NOT( ISPICKVAL( IT_Term_Type__c , &apos;&apos;)),
NOT(ISBLANK(IT_Delivery_Fixed_Day__c)),
NOT(ISBLANK(IT_Order_Reception_Term__c)),
NOT(ISPICKVAL( IT_Order_Reception_Term_Type__c, &apos;&apos; )),
NOT(ISPICKVAL( IT_Fixed_Day_Type__c, &apos;&apos; )),
 NOT(ISPICKVAL(IT_Delivery_Date_Bond__c, &apos;&apos;))
),

NOT(ISCHANGED(IT_SkipActivation__c)),
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;)

)</errorConditionFormula>
        <errorMessage>If type of contract is &apos;Quadro&apos;, you cannot modify the following fields: Delivery Term; Term Type; Delivery Fixed Day; Order Reception Term; Order Reception Term Type; Fixed Day Type, Delivery Date Bond.</errorMessage>
    </validationRules>
