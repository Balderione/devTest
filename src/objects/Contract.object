    <validationRules>
        <fullName>VRIT16_StartDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &#39;IT&#39;),
NOT(ISCHANGED(IT_SkipActivation__c)),
ISBLANK( StartDate ),
NOT(ISPICKVAL( IT_Document_Type__c , &#39;028&#39;)),
 IT_BarcodeCheck__c = false
)</errorConditionFormula>
        <errorMessage>It is necessary to insert the &#39;StartDate&#39; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT24_Contract_Renewal_Attendance</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &#39;IT&#39;),
NOT(ISCHANGED(IT_SkipActivation__c)),
$Profile.Name != &#39;System Integration&#39;,
IT_BarcodeCheck__c = false,
NOT(ISPICKVAL( IT_Document_Type__c , &#39;028&#39;)),

NOT(ISBLANK( IT_Contract_Number__c )),

OR(
 ISPICKVAL( IT_Attendance_Type__c, &quot;&quot; ),
 ISPICKVAL( IT_Contract_Renewal_Type__c, &quot;&quot; ) 

)


)</errorConditionFormula>
        <errorMessage>Attendance Type and Contract Renewal type must be populated!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT25_No_Payment_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &#39;IT&#39;),
NOT(ISCHANGED(IT_SkipActivation__c)),

NOT(ISPICKVAL( IT_Document_Type__c , &#39;028&#39;)),

ISPICKVAL( IT_Payment_Code__c , &#39;&#39;),
				IT_BarcodeCheck__c = false

)</errorConditionFormula>
        <errorMessage>Missing Payment Code!</errorMessage>
    </validationRules>
