    <validationRules>
        <fullName>VRER01_ActiveContractStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
ISPICKVAL(Status,&apos;Activated&apos;),
ISCHANGED(StartDate) 
)</errorConditionFormula>
        <errorMessage>You cannot change the Start Date when the contract is Activated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER02_ContractStatus</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
ISCHANGED(Status),
OR(
AND(
ISPICKVAL(Status,&apos;Activated&apos;),
ISBLANK( ER_Creation_date_in_OS__c )
),
AND(
ISPICKVAL(Status,&apos;Terminated&apos;),
ISBLANK( ER_Terminated_Date__c ) 
),
AND(
ISPICKVAL(Status,&apos;Activated&apos;),
ISPICKVAL(PRIORVALUE(Status),&apos;Terminated&apos;)
)
)
)</errorConditionFormula>
        <errorMessage>You cannot perform this Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER03_ContractEndDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
ISCHANGED(EndDate),
NOT(ISBLANK(EndDate)),
EndDate &lt;  StartDate 
)</errorConditionFormula>
        <errorMessage>The end date cannot be before the start date.</errorMessage>
    </validationRules>
    <validationRules>
            <fullName>VRER04_TerminationReasonMandatory</fullName>
        <active>true</active>
        <description>Mandatory when Contract End Date is not empty</description>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
ISBLANK( TEXT(ER_TerminationReason__c)),
NOT(ISBLANK( EndDate )) 
)</errorConditionFormula>
        <errorMessage>Please Capture the Termination Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT01_Discount_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
NOT(ISCHANGED(IT_SkipActivation__c)),

ISPICKVAL(IT_Discount_Type__c, &apos;4&apos;) , 
AND(
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;3&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;2&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;B&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;D&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;C&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;0V&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1J&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;0L&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;R&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1R&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;0W&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;U&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;0O&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;S&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;K&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;V&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;T&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;0M&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1A&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;W&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1Q&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;Y&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1S&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1T&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1U&apos;)),
NOT (ISPICKVAL(  IT_Financial_Center__r.IT_Service__c , &apos;1V&apos;))
))</errorConditionFormula>
