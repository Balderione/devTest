    <validationRules>
        <fullName>VRIT01_Send_SUT</fullName>
        <active>true</active>
        <errorConditionFormula>AND(		
				$User.ER_BypassVR__c = False,
ISPICKVAL(	$User.ER_Business_Unit__c , &apos;IT&apos;),
				OR(
				    ISPICKVAL( IT_Contact_Detail__r.IT_Apparatus_Type__c ,&apos;001&apos;),
	       ISPICKVAL( IT_Contact_Detail__r.IT_Apparatus_Type__c ,&apos;002&apos;)
								
				),
		  OR(
								IT_Send_SUT__c,
			     IT_Notifications__c
				)
			
)</errorConditionFormula>
        <errorMessage>&apos;Send SUT&apos; field or &apos;Notifications&apos; field cannot be selected because the Apparatus Type is &apos;Phone&apos; or &apos;Fax&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT02_Order_Confirm_Address</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
						$User.ER_BypassVR__c = False,
ISPICKVAL(	$User.ER_Business_Unit__c , &apos;IT&apos;),
				
								 ISPICKVAL( IT_Contact_Detail__r.IT_Apparatus_Type__c ,&apos;001&apos;),
					 IT_Order_Confirm_Address__c )</errorConditionFormula>
        <errorMessage>&apos;Order Confirm Address&apos; field cannot be selected because the Apparatus Type is &apos;Phone&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT03_Modify</fullName>
