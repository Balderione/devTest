    <validationRules>
        <fullName>VRER01_Case_CheckCaseReasonOnClose</fullName>
        <active>true</active>
        <description>When closing check if the fields &quot;Case Reason&quot; and &quot;Case Detailed Reason&quot;  are not empty.</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c), 
ISCHANGED(Status), 
OR( 
ISPICKVAL(Status, &quot;Resolved&quot; ) 
), 
ISPICKVAL(ER_Reason__c, &quot;&quot; ), 
ISPICKVAL(ER_CaseReason2__c, &quot;&quot; ) 
)</errorConditionFormula>
        <errorMessage>These required fields must be completed: Case Reason, Case Detailed Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER02_Case_BusinessUnit</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c), 
TEXT(ER_BUPicklist__c) != TEXT($User.ER_Business_Unit__c),
NOT(ISBLANK(ER_Business_Unit__c ))
)</errorConditionFormula>
        <errorDisplayField>ER_BUPicklist__c</errorDisplayField>
        <errorMessage>You cannot use this Business Unit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT01_Block_Provisioning_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	$User.ER_BypassVR__c = False,
	ISPICKVAL($User.ER_Business_Unit__c , &quot;IT&quot;),
 RecordType.DeveloperName = &quot;IT_Client_Welfare_Provisioning_Case_RT&quot;,
 OR(
  AND(
   TEXT(IT_Provisioning__c) = &quot;&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Details Plan Definition&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Details Plan Definition&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Activation Kit Verification&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Details Plan Definition&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Activation Kit Verification&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Master Data Activation&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Details Plan Definition&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Activation Kit Verification&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Master Data Activation&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Circuit Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Details Plan Definition&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Activation Kit Verification&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Master Data Activation&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Circuit Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Plan Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Details Plan Definition&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Activation Kit Verification&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Master Data Activation&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Circuit Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Plan Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Comunication To Client Sending&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Take Charge&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Details Plan Definition&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Activation Kit Verification&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Master Data Activation&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Circuit Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Plan Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Comunication To Client Sending&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;&quot;
  ),
  AND(
   OR(
    TEXT(IT_Provisioning__c) = &quot;Circuit Configuration&quot;,
    TEXT(IT_Provisioning__c) = &quot;Plan Configuration&quot;,
    TEXT(IT_Provisioning__c) = &quot;Comunication To Client Sending&quot;
   ),
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Circuit Configuration&quot;,
   TEXT(IT_Financial_Center__r.IT_Client_Status__c) &lt;&gt; &quot;02&quot;
  ),
  AND(
   TEXT(IT_Provisioning__c) = &quot;Plan Configuration&quot;,
   TEXT(PRIORVALUE(IT_Provisioning__c)) &lt;&gt; &quot;Plan Configuration&quot;,
   ISBLANK(IT_Financial_Center__r.IT_Circuit__c)
  )
 )
)</errorConditionFormula>
        <errorMessage>You can&apos;t change Provisioning Status in this way</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT02_Case_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

 $User.ER_BypassVR__c = False,
 ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),

 ISPICKVAL( Status ,&apos;Closed&apos;),
 ISBLANK(TEXT( IT_Case_Reason__c )),
  RecordType.DeveloperName  &lt;&gt; &quot;IT_Client_Welfare_Provisioning_Case_RT&quot;			

)</errorConditionFormula>
        <errorMessage>You need to enter the field Case Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT03_Case_Origin</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 $User.ER_BypassVR__c = False,
 ISPICKVAL($User.ER_Business_Unit__c , &quot;IT&quot;),
 NOT(ISNEW()),
 ISCHANGED(Origin)
)</errorConditionFormula>
        <errorMessage>You can&apos;t change field &apos;Origin&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER03_Asset_Case</fullName>
        <active>true</active>
        <description>Asset information is obligatory if the case reason equals &quot;Card&quot;</description>
        <errorConditionFormula>AND(RecordType.DeveloperName = &quot;ER_User_Case_RT&quot;, 
ISPICKVAL(ER_Reason__c ,&quot;ER_Card&quot;),
ISBLANK (AssetId ),
(NOT($User.ER_BypassVR__c)))</errorConditionFormula>
        <errorMessage>Information about Asset is obligatory in order to register the Case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER04_Case_Reopening</fullName>
        <active>true</active>
        <description>Case can be reopen only with the status &quot;Reopen&quot;</description>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
ISPICKVAL(PRIORVALUE( Status),&apos;Resolved&apos;),
ISCHANGED(Status), 
NOT(ISPICKVAL( Status ,&quot;Reopen&quot;)
))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>You can only reopen the case with correct value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER05_Case_Queue_BusinessUnit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
     !ISNEW(),
     BEGINS(OwnerId, &quot;00G&quot;),
     ISCHANGED(OwnerId),
     AND(
          MID(Owner:Queue.Name, 3, 2)  !=  $User.ER_BUText__c ,
          LEFT(Owner:Queue.Name, 2)  !=  $User.ER_BUText__c
     )
)</errorConditionFormula>
        <errorMessage>Cannot assign to a Queue that is not belonging to your BU</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT04_Documentation_Outcome</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				OR(
				ISPICKVAL( IT_Provisioning__c , &apos;Creation of a new Code&apos;) ,
								ISPICKVAL( IT_Provisioning__c , &apos;Master Data Activation&apos;),
								ISPICKVAL( IT_Provisioning__c , &apos;Circuit Configuration&apos;),
								ISPICKVAL( IT_Provisioning__c , &apos;First Order Cards&apos;),
								ISPICKVAL( IT_Provisioning__c , &apos;Comunication To Client Sending&apos;)
								),
			   ISBLANK(TEXT(IT_Documentation_Outcome__c)),
				 $RecordType.Name = &quot;IT Client Consip Case RT&quot;
)</errorConditionFormula>
        <errorMessage>The &apos;Documentation Outcome&apos; field must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT05_Refused</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				OR(
					 ISPICKVAL(IT_Documentation_Outcome__c 	,&apos;&apos;) ,
								ISPICKVAL(IT_Documentation_Outcome__c 	,&apos;Refused&apos;)
								),
				 ISCHANGED(IT_Provisioning__c ),
$RecordType.Name = &quot;IT Client Consip Case RT&quot;
)</errorConditionFormula>
        <errorMessage>the &apos;Documentation Outcome&apos; field is Refused or empty, you can&apos;t change status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT05_Statement</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
			
				 ISBLANK(TEXT(IT_Statement__c )) ,
				OR(AND(
				 ISNEW(),
					$RecordType.Name = &quot;IT Client Consip Case RT&quot;
				), 
				AND(
								$RecordType.Name = &quot;IT Client Support Case RT&quot;,
	    CONTAINS(	TEXT(IT_Case_Reason__c),&apos;CONSIP&apos;) 
								))
)</errorConditionFormula>
        <errorDisplayField>IT_Statement__c</errorDisplayField>
        <errorMessage>It is necessary to insert the &apos;Statement&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT06_New_Customer_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				 IT_Reissue__c =true,
			  ISBLANK(IT_New_Customer_Code__c 	) 
				)</errorConditionFormula>
        <errorMessage>If &apos;reissue&apos; field is flagged,you have to enter &apos;New Customer Code&apos; type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT07_ReasonforSupport</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$RecordType.Name = &quot;IT Client Support Case RT&quot;,
	CONTAINS(	TEXT(IT_Case_Reason__c),&apos;CONSIP&apos;) ,
				ISBLANK(TEXT(IT_Statement__c )) 
				 
			

)</errorConditionFormula>
        <errorMessage>It is necessary to insert the &apos;Statement&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT08_Documentation_Outcome</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$RecordType.Name = &quot;IT Client Consip Case RT&quot;,
 ISCHANGED( IT_Documentation_Outcome__c ),
				NOT( ISPICKVAL(IT_Provisioning__c,&apos;Forms Acceptance&apos; )) 


)</errorConditionFormula>
        <errorMessage>You cannot change the &apos;Documentation Outcome&apos; in this Provisioning Status</errorMessage>
    </validationRules>
