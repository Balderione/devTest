    <pluralLabel>Delivery Sites</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>VRER01_DeliverCenterOnlyClientFCenter</fullName>
        <active>true</active>
        <description>Delivery Site can only be created on Client Financial Center</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c), 
TEXT(ER_Financial_Center__r.ER_Type__c) = &quot;Merchant&quot; 
)</errorConditionFormula>
        <errorMessage>Delivery Site can only be created on Client Financial Center</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT02_Delivery_Site_Editing_in_AC</fullName>
        <active>true</active>
        <errorConditionFormula>AND (NOT (ISNEW()) ,
					ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
$Profile.Name != &apos;IT System Integration&apos;,
IT_Delivery_AC__c == null,
NOT($User.ER_BypassVR__c),
NOT(ISCHANGED(IT_Count_Distribution_Point__c)))</errorConditionFormula>
        <errorMessage>Sincronizzazione Anagrafica Clienti in corso: non è possibile modificare la consegna!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT03_Modify</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
$Profile.Name != &apos;IT System Integration&apos;,
NOT(
OR(
ISPICKVAL(ER_Financial_Center__r.IT_Service__c  , &apos;1B&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;1X&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;2X&apos;),
ISPICKVAL(ER_Financial_Center__r.IT_Service__c  , &apos;0G&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;0F&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;0H&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;0P&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;0Q&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;1W&apos;),
ISPICKVAL(ER_Financial_Center__r.IT_Service__c , &apos;2F&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;2G&apos;),
ISPICKVAL(ER_Financial_Center__r.IT_Service__c  , &apos;1D&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;1L&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;1M&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c , &apos;1N&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;1O&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c , &apos;1E&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c , &apos;1Y&apos;),
ISPICKVAL(ER_Financial_Center__r.IT_Service__c  , &apos;1H&apos;),
ISPICKVAL(ER_Financial_Center__r.IT_Service__c  , &apos;2J&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;2X&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Service__c  , &apos;2C&apos;)/*,
AND(  
NOT(ISBLANK(ER_Financial_Center__r.IT_DEMO_Financial_Center__c )),
ISPICKVAL(ER_Financial_Center__r.IT_Service__c  , &apos;1B&apos;)
)*/
)
))</errorConditionFormula>
        <errorMessage>Impossible to modify for this type of service</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT04_Status_Based_Change_Block</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
$Profile.Name != &apos;IT System Integration&apos;,
OR(
ISPICKVAL( ER_Financial_Center__r.IT_Client_Status__c , &apos;10&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Client_Status__c , &apos;00&apos;),
ISPICKVAL( ER_Financial_Center__r.IT_Client_Status__c , &apos;03&apos;)
)
)</errorConditionFormula>
        <errorMessage>Impossible to modify fields for this client status!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT05_Modify_Note</fullName>
        <active>true</active>
