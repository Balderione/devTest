    <validationRules>
        <fullName>VRIT01_Funding_Plan_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),

OR
(IT_Membership_Choice_Start_Date__c &lt; IT_Plan__r.IT_Plan_Start_Date__c,
IT_Membership_Choice_End_Date__c  &gt; IT_Plan__r.IT_Plan_End_Date__c ))</errorConditionFormula>
        <errorMessage>Funding Source&apos;s dates must be included in Plan&apos;s dates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT02_Funding_Start_and_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				IT_Membership_Choice_End_Date__c &lt; IT_Membership_Choice_Start_Date__c,
				NOT($User.ER_BypassVR__c),
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;))</errorConditionFormula>
        <errorMessage>Membership Choice End Date must be after Membership Choice Start Date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT03_Beneficiaries</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
 
IT_Beneficiaries__c &lt;IT_Beneficiaries_Member__c 
)</errorConditionFormula>
        <errorMessage>Beneficiaries member cannot be more than Beneficiaries!</errorMessage>
    </validationRules>
