    <validationRules>
        <fullName>VR04IT_Fiscal_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
						$User.ER_BypassVR__c = False,
ISPICKVAL(	$User.ER_Business_Unit__c , &apos;IT&apos;),
NOT(IT_Account__r.IT_Fiscal_Code__c = IT_Fiscal_Code__c)

)</errorConditionFormula>
        <errorMessage>The &apos;Fiscal Code&apos; must correspond to an existing account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT01_Validity_Year</fullName>
        <active>true</active>
        <errorConditionFormula>AND($User.ER_BypassVR__c = False,
	ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
				NOT(ISNUMBER(IT_Validity_Year__c)) ||
(ISNUMBER(IT_Validity_Year__c) &amp;&amp;
ISCHANGED(IT_Validity_Year__c) &amp;&amp;
VALUE(PRIORVALUE(IT_Validity_Year__c)) &gt; VALUE(IT_Validity_Year__c)))</errorConditionFormula>
        <errorMessage>WARNING! Enter the current year or current year +1!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT02_Presentation_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				IT_Presentation_Date__c &gt;TODAY(),
				$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;)
)</errorConditionFormula>
        <errorMessage>It cannot be a future date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT03_Exporter_Type_OR</fullName>
        <active>true</active>
        <errorConditionFormula>AND(	$User.ER_BypassVR__c = False,
ISPICKVAL(	$User.ER_Business_Unit__c , &apos;IT&apos;),
		OR(		AND(
 			NOT( ISBLANK(IT_Declaration_Number__c  )),
				ISPICKVAL( IT_Operation_Type__c	  ,&apos;&apos;)),
				AND(
											NOT( ISBLANK(IT_Declaration_Number__c  )),
			ISPICKVAL( IT_Exporter_Type__c ,&apos;&apos;)),
								AND(
											NOT( ISBLANK(IT_Declaration_Number__c  )),
			ISBLANK(  IT_Declared_Amount__c )),
					AND(
									NOT( ISBLANK(IT_Declaration_Number__c )),
ISBLANK( IT_Fiscal_Code__c )
	
								)
)
			)</errorConditionFormula>
        <errorMessage>Insert fied &apos;Exporter Type&apos; or &apos;Operation Type&apos; or &apos;Declaration Amount&apos; or &apos;Fiscal Code&apos;.</errorMessage>
    </validationRules>
