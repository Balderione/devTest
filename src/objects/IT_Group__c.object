    <validationRules>
        <fullName>VRIT01_Modify</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
NOT(
OR( 
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;1B&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;1X&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;2X&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;0G&apos;),
ISPICKVAL( IT_Parent_Company__r.IT_Service__c , &apos;0F&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;0H&apos;),
ISPICKVAL( IT_Parent_Company__r.IT_Service__c , &apos;0P&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c, &apos;0Q&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c, &apos;1W&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c, &apos;2F&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;2G&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c, &apos;1D&apos;),
ISPICKVAL( IT_Parent_Company__r.IT_Service__c , &apos;1L&apos;),
ISPICKVAL( IT_Parent_Company__r.IT_Service__c , &apos;1M&apos;),
ISPICKVAL( IT_Parent_Company__r.IT_Service__c , &apos;1N&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;1O&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;1E&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c, &apos;1Y&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c, &apos;1H&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;2J&apos;),
ISPICKVAL(IT_Parent_Company__r.IT_Service__c , &apos;2X&apos;)/*,
AND(
NOT(ISBLANK( IT_Parent_Company__r.IT_DEMO_Financial_Center__c  )),
