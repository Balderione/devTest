    <validationRules>
        <fullName>VRER01_Quote_CheckChangeStatus</fullName>
        <active>true</active>
        <description>Quote status management.
If denied ==&gt; cannot be changed
if accepted ==&gt; cannot change to Presented pr Draft</description>
        <errorConditionFormula>AND(
 NOT($User.ER_BypassVR__c),
 OR(
  AND(
   ISPICKVAL(PRIORVALUE( Status ) , &quot;Accepted&quot;),
   OR(
    ISPICKVAL( Status , &quot;Presented&quot;),
    ISPICKVAL( Status , &quot;Draft&quot;)
   )
  ),
  AND(
   ISPICKVAL(PRIORVALUE( Status ) , &quot;Denied&quot;),
   ISCHANGED(Status)
 )
 )
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>This status cannot be used !</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER02_Quote_ClosedWonBlockEdit</fullName>
        <active>true</active>
        <description>When stage is set to “Closed won”, the opportunity record and related quotes cannot be edited.</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c), 
TEXT(Opportunity.StageName)=&apos;Closed won&apos;, 
NOT(ISCHANGED( LastModifiedDate )) 
)</errorConditionFormula>
        <errorMessage>When stage is set to “Closed won”, the opportunity record and related quotes cannot be edited.</errorMessage>
    </validationRules>
