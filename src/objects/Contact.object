    <validationRules>
        <fullName>VRER01_Email_OR_PhoneNumber_required</fullName>
        <active>false</active>
        <description>Suggestion: E-mail or Phone/Mobilephone number must be a required field.</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c),
AND( 
Email == null, 
AND (Phone == null, MobilePhone == null) 
))</errorConditionFormula>
        <errorMessage>E-mail or Phone/Mobile phone number are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRER03_Contact_Mandatory_Fields</fullName>
        <active>true</active>
        <description>The field FirstName is mandatory.</description>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
OR(
ISBLANK (FirstName)))</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>The first name information is mandatory to create a contact</errorMessage>
    </validationRules>
    <validationRules>
    <fullName>VRER04_Inactivation_Reason_Mandatory</fullName>
        <active>true</active>
        <description>When the Contact becomes inactive, the inactive reason becomes mandatory.</description>
        <errorConditionFormula>AND(
NOT($User.ER_BypassVR__c),
OR( AND(ISPICKVAL( ER_Status__c ,&apos;Inactive&apos; ),
ISBLANK( TEXT(ER_Inactive_Reason__c) )) , 
AND(ISPICKVAL( ER_Status__c ,&apos;Active&apos; ),
NOT(ISBLANK( TEXT(ER_Inactive_Reason__c)))) )
)</errorConditionFormula>
        <errorMessage>The Inactivation Reason should be filled when the contact is Inactive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRPL01_Email_Or_PhoneNumber_Required</fullName>
        <active>false</active>
        <description>E-mail or Phone/Mobilephone number must be filled in.</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c),
ER_Business_Unit__c =&apos;PL&apos;, 
Email == null,
Phone == null,
MobilePhone == null 
)</errorConditionFormula>
        <errorMessage>E-mail or Phone/Mobile phone number are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRHU04_Mobile_Number_Hungarian_Format</fullName>
        <active>true</active>
        <description>This validation rule will control the phone number format to ensure it fit Hungarian format, as following : +36-11-111-1111 (mobile phones)</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c),
ER_Business_Unit__c =&apos;HU&apos;,  
NOT( 
OR( 
LEN( MobilePhone ) = 0, 
REGEX( MobilePhone , &quot;[+36]{3}-[0-9]{2}-[0-9]{3}-[0-9]{4}&quot;) 
)))</errorConditionFormula>
        <errorDisplayField>MobilePhone</errorDisplayField>
        <errorMessage>The format of the Mobile Phone Number must be equal to Hungarian format, as following : 
+36-11-111-1111 (mobile phones)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRHU05_Phone_Number_Hungarian_Format</fullName>
        <active>true</active>
        <description>This validation rule will control the phone number format to ensure it fit Hungarian format, as following : 
+36-1-111-1111 (capital city)
+36-11-111-111 (countryside)</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c),
ER_Business_Unit__c =&apos;HU&apos;,  
NOT( 
OR( 
LEN(  Phone ) = 0, 
REGEX(  Phone , &quot;[+36]{3}-[0-9]{1}-[0-9]{3}-[0-9]{4}&quot;),
REGEX( Phone , &quot;[+36]{3}-[0-9]{2}-[0-9]{3}-[0-9]{3}&quot;) 
)))</errorConditionFormula>
        <errorDisplayField>Phone</errorDisplayField>
        <errorMessage>The format of the Phone Number must be equal to Hungarian format, as following : 
+36-1-111-1111 (capital city)
+36-11-111-111 (countryside)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRHU10_Email_OR_PhoneNumber_required</fullName>
        <active>false</active>
        <description>Suggestion: E-mail or Phone/Mobilephone number must be a required field.</description>
        <errorConditionFormula>AND( 
NOT($User.ER_BypassVR__c), 
ER_Business_Unit__c =&apos;HU&apos;,   
AND(
Email == null,
AND (Phone == null, MobilePhone == null)  
))</errorConditionFormula>
        <errorMessage>E-mail or Phone/Mobile phone number are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT01_Contact_Detail_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$User.ER_BypassVR__c = False,
ISPICKVAL( $User.ER_Business_Unit__c , &apos;IT&apos;),
NOT(ISBLANK(Phone)),
NOT( REGEX( Phone , &quot;[\\s0-9]+&quot;))

)</errorConditionFormula>
        <errorMessage>Phone characters must be numbers!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT02_Client_SOC</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
$Profile.Name != &apos;IT System Integration&apos;,
				 ISPICKVAL(IT_Type__c	, &apos;Soc&apos;))</errorConditionFormula>
        <errorMessage>&apos;Soc&apos; type customer is not possible to make changes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRIT03_Fax_Length</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
$User.ER_BypassVR__c = False,
ISPICKVAL($User.ER_Business_Unit__c , &apos;IT&apos;),
LEN(Fax) &gt; 16 
 )</errorConditionFormula>
        <errorMessage>Fax cannot be longer than 16 characters.</errorMessage>
    </validationRules>
