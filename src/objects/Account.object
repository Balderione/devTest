    <validationRules>
        <fullName>VRIT08_SDILen</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 $User.ER_BypassVR__c = False,
 ISPICKVAL($User.ER_Business_Unit__c , &#39;IT&#39;),

 IT_VAT_Group_SDI__c != null,

 OR(
  AND(
  
    ISPICKVAL ( IT_Company_Type__c , &#39;Institution&#39;),
   OR(
    LEN (IT_VAT_Group_SDI__c) &lt; 6,
    LEN (IT_VAT_Group_SDI__c) &gt; 6
   )
  ),
  AND(
OR(
  ISPICKVAL( IT_Company_Type__c, &#39;Private (work for Institution)&#39;),
   ISPICKVAL ( IT_Company_Type__c , &#39;Private&#39;)),
   OR(
    LEN (IT_VAT_Group_SDI__c) &lt; 7,
    LEN (IT_VAT_Group_SDI__c) &gt; 7
   )
  )
 )
)</errorConditionFormula>
        <errorMessage>If Company Type is Institution or Private works for Institution, VAT Group SDI must contains 6 characters!
If Company Type is Private, VAT Group SDI must contains 7 characters!</errorMessage>
    </validationRules>
